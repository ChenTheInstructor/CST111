<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="413.0" prefWidth="479.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="p5.LoginController">
   <children>
      <TextField fx:id="usernameTF" layoutX="161.0" layoutY="98.0" promptText="Enter user" />
      <Label layoutX="104.0" layoutY="102.0" text="User" />
      <Label layoutX="104.0" layoutY="134.0" text="Password" />
      <Button fx:id="loginBtn" layoutX="213.0" layoutY="182.0" mnemonicParsing="false" onAction="#onLoginBtnClicked" text="Login" />
      <Label layoutX="176.0" layoutY="238.0" text="Don't have an accoiunt?" />
      <Button fx:id="signUpBtn" layoutX="210.0" layoutY="262.0" mnemonicParsing="false" onAction="#onSignUpBtnClicked" text="Sign Up" />
      <PasswordField fx:id="passwordTF" layoutX="161.0" layoutY="130.0" promptText="Enter password" />
      <Label fx:id="errorLabel" layoutX="158.0" layoutY="59.0" prefHeight="17.0" prefWidth="156.0" textFill="RED" />
   </children>
</AnchorPane>
